        "start:dev": "env-cmd -f .env.development vite --mode development",
        "start:prod": "env-cmd -f .env.production vite --mode production",
        "prod": "vite --mode production",
        "build:dev": "env-cmd -f .env.development vite build --mode development",
        "build:prod": "env-cmd -f .env.production vite build --mode production",
        "build": "vite build --mode production",
        "preview": "vite preview --mode production",
        "lint": "eslint .",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "client-start-dev": "vite --mode development",
        "client-build-dev": "vite build --mode development",
        "client-start-prod": "vite --mode production",
        "client-build-prod": "vite build --mode production",
        "deploy-ghpages": "gh-pages --mode production",
        "client-preview-prod": "vite preview --mode production --port 8080",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f",
        "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
        "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
        "deploy:client": "cd client && npm run build && docker build -t mindspace-client ."
